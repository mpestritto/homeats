
<div class="grid12_6 prefix_6 header_login">
  <% if user_signed_in? %>
    Signed in as <%= current_user.email %>. Not you?
    <%= link_to "Logout", destroy_user_session_path %>
  <% else %>
    <%= link_to "Login", new_user_session_path  %> | <%= link_to "Register", new_user_registration_path %>
  <% end %>
  <span>| <a href="mailto:team@saucychefs.com">Contact us</a></span>
</div>
<div class="clear"></div>
<div class="header_whitespace"><%= link_to ( image_tag "logo.png" , :class=>'header_logo'), :controller=>:home, :action=>:index %>
<div id="div_flash">
  <div id="notice"><%= notice %></div>
  <div id="alert"><%= alert %></div>
</div>
</div>
<div class="clear"></div>
